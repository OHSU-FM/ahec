<% ur = ua.user_responses.first %>
<ul class="list-group">
  <li class="list-group-item">
  <div class="row">
    <div class="col-md-8">
      <%= ur.title %>
    </div>
    <div class="col-md-2">
      <span class="label label-danger"><%= ur.status %></span>
    </div>
    <div class="col-md-2">
      <%= link_to assignment_assignment_comments_path(user_assignment_id: 1),
              data: {toggle: :modal} do %>
          <i class='fa fa fa-comments-o'></i>
      <% end %>
    </div>
  </div>
  </li>
</ul>
