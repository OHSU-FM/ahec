<div class="col-sm-2">
   <table id="CompTable" class="table borderless table-condensed table-striped table-hover ls_reports_filter spreadsheet-body">
      <tr>
          <th><center>Competency</center></th>
          <th><center>Average Complete (%)</center></th>
      </tr> 

      <% @comp_class_mean.each do |k,v| %>
          <tr>
              <td><center>
                  <%= k %>
              </center>
              </td>
              <td><center><%= v %></center></td>
          </tr>
      <% end %>
   </table>    
</div>  

<div class="col-sm-6">
    <div id="data-visualization-all-comp" style="min-width: 1000px; height: 600px; margin: 0 auto"></div>
    <div id="comp-graph-buttons">
    <button id="plain1" class="btn btn-info btn-sm">Column</button>
    <button id="inverted1" class="btn btn-info btn-sm">Bar</button>

    </div>
</div>  